{
    "name": "bbc/programmes-pages-service",
    "license": "proprietary",
    "type": "library",
    "description": "A library for building /programmes pages",
    "autoload": {
        "psr-4": { "BBC\\ProgrammesPagesService\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\BBC\\ProgrammesPagesService\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bbc/DoctrineExtensions.git"
        }
    ],
    "require": {
        "php": ">=7.0.1",
        "doctrine/orm": "^2.5",
        "bbc/doctrine-extensions": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "squizlabs/php_codesniffer": "^2.5",
        "escapestudios/symfony2-coding-standard": "^2.0",
        "doctrine/data-fixtures": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.0.1"
        }
    }
}
