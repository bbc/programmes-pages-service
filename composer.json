{
    "name": "bbc/programmes-pages-service",
    "license": "Apache-2.0",
    "type": "library",
    "description": "A library for building /programmes pages",
    "autoload": {
        "psr-4": { "BBC\\ProgrammesPagesService\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\BBC\\ProgrammesPagesService\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bbc/DoctrineExtensions.git"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "bbc/doctrine-extensions": "^2.4.137",
        "doctrine/orm": "^2.5",
        "psr/cache": "^1.0",
        "psr/log": "^1.0",
        "symfony/stopwatch": "^3.2"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2",
        "escapestudios/symfony2-coding-standard": "^2.9",
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^2.9",
        "symfony/cache": "^3.2"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "sort-packages": true
    }
}
